package br.com.Caellum.OrientacaoObjetos;

public class Funcionario {

	String nome;
	String departamento;
	double salario;
	String data;
	String rg;
	
	
	public void recebeAumento(double valor){
		this.salario = this.salario + valor;
		
	}
	
	double calculoGanhoAtual(){
		return this.salario * 12;
		
	}
	
}
