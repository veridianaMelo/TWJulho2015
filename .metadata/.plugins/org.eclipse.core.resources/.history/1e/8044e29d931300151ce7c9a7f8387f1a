package br.com.Caellum.Variáveis3;

public class Exercício4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

			
		int fatorial = 1;		
		for(int i=1; i <= 10; i++){
			while(i > 2){
				fatorial = fatorial * 2;
				break;
				
			}System.out.println("Fatorial de "+i+" = "+fatorial);
			
				}
	}

}
