package br.com.Caellum.OrientacaoObjetos;

public class Conta {


	int numero;
	String dono;
	double saldo;
	double limite;


	boolean sacar(double valor){
		if(valor > this.saldo){
			return false;
		}else{
			this.saldo =  this.saldo - valor;
			return true;

		}
	}

	public void deposita(double valor){
		this.saldo +=  valor;


	}

}
