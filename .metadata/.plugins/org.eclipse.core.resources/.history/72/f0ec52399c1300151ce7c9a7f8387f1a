package br.com.Caellum.Variáveis3;

public class Fibonacci {

	static long fibo(int n){
		if (n < 2){
			return n;
		}else{
			return fibo(n-1) + (n - 2);
		}
	}


	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i = 0; i < 30; i++){
			System.out.print("(" + i + "):" + Fibonacci.fibo(i) + "\t");


		}
	}
}






