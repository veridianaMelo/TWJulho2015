package br.com.Edmodo.ExercicioII;

public class Exercicio1c {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] array = new int[10];
		
		for(int i=0; i < array.length; i++){
			
			if(i < 5){
			array[i] = i + 1;
			System.out.println(array[i]);
		}else if(i > 5) {
			array[i] = (i * 2);
			System.out.println(array[i]);
		}
	}

	}}
